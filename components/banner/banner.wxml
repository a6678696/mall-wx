<!--components/banner/banner.wxml-->
<view class="banner">
    <swiper indicator-dots indicator-active-color="#ff6200" circular autoplay interval="3000">
        <swiper-item wx:for="{{carouselImageList}}" wx:key="id">
            <navigator url="{{item.iamgeUrl}}">
                <image mode="widthFix" src="{{item.image}}"></image>
            </navigator>
        </swiper-item>
    </swiper>
</view>